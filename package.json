{
  "name": "react-electron",
  "version": "1.0.0",
  "description": "",
  "main": "build/electron.js",
  "build": {
    "asar": true,
    "appId": "com.app.reactelectron",
    "productName": "reactelectron",
    "extends": "react-cra",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "build"
  },
    "win": {
      "icon": "build/assets/clock-icon_256x256.png",
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "scripts": {
    "build": "webpack",
    "react:dev": "webpack serve --mode=development",
    "electron:dev": "nodemon",
    "dev": "concurrently --kill-others \"npm run react:dev\" \"npm run electron:dev\"",
    "start": "npm run build && electron build/electron.js",
    "deploy": "electron-builder --windows zip:x64"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "asar": "^3.0.3",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.6",
    "electron": "^13.1.4",
    "electron-builder": "^22.11.7",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^1.5.0",
    "html-webpack-plugin": "^5.3.2",
    "nodemon": "^2.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
